<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.ControleDeEstoque" parent="Theme.Material3.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryVariant</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorSecondaryVariant">@color/colorSecondaryVariant</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorOnPrimaryContainer">@color/colorOnPrimaryContainer</item>
        <item name="colorPrimaryContainer">@color/colorPrimaryContainer</item>
        <item name="colorSecondaryContainer">@color/colorSecondaryContainer</item>
        <item name="colorOnSecondaryContainer">@color/white</item>
        <item name="android:shadowColor">@color/colorSecondary</item>
        <item name="colorContainer">@color/colorSecondary</item>
        <item name="android:textColorHighlight">@color/color_100</item>

        <!-- Status bar and Navigation bar color. -->
        <item name="navigationIconTint">@color/spacegray</item>
        <item name="android:navigationBarColor">@color/colorBackground</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:statusBarColor">@color/colorBackground</item>
        <item name="android:windowBackground">@color/colorBackground</item>

    </style>
</resources>