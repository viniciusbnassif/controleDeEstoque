<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">    <!-- Base application theme. -->
    <style name="Theme.ControleDeEstoque" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">#004C69</item>
        <item name="colorPrimaryDark">#3700B3</item>
        <item name="colorAccent">#4081ff</item>
        <item name="colorPrimaryVariant">#00668B</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#4BB6E8</item>
        <item name="colorSecondaryVariant">#007FAC</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorOnPrimaryContainer">#004C69</item>
        <item name="colorPrimaryContainer">@color/color_0</item>
        <item name="colorSecondaryContainer">#004C69</item>
        <item name="colorOnSecondaryContainer">@color/white</item>
        <item name="android:shadowColor">#4BB6E8</item>
        <item name="colorContainer">#4BB6E8</item>
        <item name="android:textColorHighlight">@color/color_100</item>
        <item name="android:popupBackground">@color/color_100</item>
        <item name="android:popupCharacters">@color/color_100</item>

        <!-- Status bar and Navigation bar color. -->
        <item name="navigationIconTint">@color/spacegray</item>
        <item name="android:navigationBarColor">@color/colorBackground</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/colorBackground</item>
        <item name="android:windowBackground">@color/colorBackground</item>





    </style>

</resources>